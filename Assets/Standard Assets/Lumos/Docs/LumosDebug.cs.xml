<?xml version="1.0"?>
<doc>
    <assembly>
        <name>LumosDebug</name>
    </assembly>
    <members>
        <member name="T:Lumos">
            <summary>
            Replacement Debug functions that the library can use without being picked
            up by Lumos' Diagnostics powerup.
            </summary>
        </member>
        <member name="M:Lumos.Log(System.Object)">
            <summary>
            Records a debug message.
            </summary>
            <param name="message">The message to log.</param>
        </member>
        <member name="M:Lumos.LogWarning(System.Object)">
            <summary>
            Records a warning.
            </summary>
            <param name="message">The message to log.</param>
        </member>
        <member name="M:Lumos.LogError(System.Object)">
            <summary>
            Records an error.
            </summary>
            <param name="message">The message to log.</param>
        </member>
        <member name="M:Lumos.LogMessage(Lumos.MessageLogger,System.Object)">
            <summary>
            Records a message.
            </summary>
            <param name="logger">The function to send the message to.</param>
            <param name="message">The message to log.</param>
        </member>
    </members>
</doc>
